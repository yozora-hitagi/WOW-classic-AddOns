[Script]
Dim FindX
Dim FindY
Function Find(x1, y1, x2, y2, colors, t, p)
    For i = 0 To UBound(colors)
        FindColorEx x1, y1, x2, y2, colors(i), t, p, x, y
        If x > 0 And y > 0 Then
            Find = i
            FindX = x
            FindY = y
            //            MessageBox FindX
            Exit Function
        End If
    Next
    Find = -1
    FindX = x
    FindY = y
End Function




