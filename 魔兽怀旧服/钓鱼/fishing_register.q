//字符串加解密， 相同字符操作 5个， 解密会失败。 不晓得为啥

Function AppendToInputBox3(str)
    If Form1.InputBox3.Text = "" Then 
        Form1.InputBox3.Text = str
    Else 
        Form1.InputBox3.Text = Form1.InputBox3.Text & Chr(13) & Chr(10) & Chr(13) & Chr(10) & str
    End If
End Function
Event Form1.Load
    Form1.InputBox1.Text =  Lib.Yozoraの命令库.MachineCode()
    返回值 = Lib.网络.获取网络时间_增强版("www.taobao.com")
    AppendToInputBox3 (返回值)
End Event
Event Form1.Button_GEN.Click
    mc = Form1.InputBox1.Text
    mc = Trim(mc)
    
    Timeout = Int(Form1.InputBox2.Text)
    If Timeout = "" Then 
        MessageBox "请输入正确的有效期！"
    End If
    
    AppendToInputBox3("保存文件 " & Form1.BrowseBox1.Path )
    返回值 = Lib.Yozoraの命令库.生成License(mc,"FISH",Timeout,Form1.BrowseBox1.Path )
    AppendToInputBox3 ("生成License 到期日期：" & 返回值)
End Event
Event Form1.Button_Verify.Click
    mc = Form1.InputBox1.Text
    mc = Trim(mc)
    
    AppendToInputBox3("读取文件 " & Form1.BrowseBox1.Path )
    返回值 = Lib.Yozoraの命令库.验证License(mc,"FISH",Form1.BrowseBox1.Path)
    AppendToInputBox3 ("验证License 到期时间：" & 返回值)
    
    TracePrint VarType(返回值)
    TracePrint TypeName(返回值)
End Event
