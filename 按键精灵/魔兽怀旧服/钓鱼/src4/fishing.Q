Function RegistCode()
    GetSN = Plugin.Sys.GetHDDSN()
    RegistCode = Plugin.Encrypt.Md5String(GetSN)
End Function


Function OutDate(license)


    y = Int(license(1))
    m = Int(license(2))
    d = Int(license(3))
    
    If y = "" Or m = "" Or d = "" Then 
        OutDate = 2
        Exit Function
    End If

	
    返回值 = Lib.网络.获取网络时间_增强版("www.baidu.com")
    If 返回值 = "" Then 
        cy = Year(Date)
        cm = Month(Date)
        cd = Day(Date)
    Else 
        cy = Year(返回值)
        cm = Month(返回值)
        cd = Day(返回值)
    End If
	

	
    If y > cy Then 
        OutDate = 0
        Exit Function
    End If
    
    If y = cy And m > cm Then 
        OutDate = 0
        Exit Function
    End If
	
    If y = cy And m = cm And d >= cd Then 
        OutDate = 0
        Exit Function
    End If
	
    OutDate = 1
	
End Function


Function LOG(logstr)
    FishForm.InputBoxLOG.Text = FishForm.InputBoxLOG.Text & Date & " " & logstr & Chr(13) & Chr(10)
End Function


Function CheckLicense()
	
	
    //------------------------------------------------
    //license 检查
    //------------------------------------------------

    //读取license
    lic_handle = Plugin.File.OpenFile("fishing.lic")
    license = Plugin.File.ReadLine(lic_handle)
    Call Plugin.File.CloseFile(lic_handle)
    
    license = Trim(license)
    license = Plugin.Encrypt.StringDecode(license, "whoisyourdaddy")
    license = Split(license, "|")


    FishForm.Label7.Caption = "未注册"
    FishForm.Label7.BackColor = "0000FF"
    CheckLicense = 0
    
    If license(0) = RegistCode() Then 
        If OutDate(license) = 1 Then 
            FishForm.Label7.Caption="已过期"
            FishForm.Label7.BackColor = "00FFFF"
            CheckLicense = 1
            
        ElseIf OutDate(license) = 0 Then
            FishForm.Label7.Caption="已注册"
            FishForm.Label7.BackColor = "00FF00"
            CheckLicense = 2
            
        End If 
    End If
    FishForm.Label9.Caption = "有效期至：" & license(1) & "-" & license(2) & "-" & license(3)
	
	
	
End Function


