    End If  

 
End Event  



Function NaturalNumber(txt)
    newtext = txt
    For i = 1 To len(txt) 
        tt = Asc(mid(txt, i, 1))
        If tt < 48 or tt > 57 Then 
            newtext = Replace(newtext, Chr(tt), "")

        End If
    Next
    NaturalNumber=newtext
End Function


Event FishForm.InputBox1.Change   

    FishForm.InputBox1.Text=NaturalNumber(FishForm.InputBox1.Text)

End Event


